<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cc.stock.dao.DelegateDao">
    <insert id="addDelegate" parameterType="com.cc.stock.entity.Delegate">

        INSERT INTO delegate (user_id,stock_id,price,num,deal,type,time) VALUE (#{userId},#{stockId},#{price},#{num},#{deal},#{type},#{time})

    </insert>
</mapper>